{
  "version": 2,
  "env": {
    "NODE_ENV": "production",
    "REPL_ID": ""
  },
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "node scripts/build-vercel.js"
}